    "babel-plugin-add-module-exports": "*",
    "babel-preset-es2015": "*",
    "babel-preset-stage-0": "*",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-loader": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
